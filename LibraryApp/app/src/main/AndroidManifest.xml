<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bochi91.libraryapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewBooksActivity"
            android:label="@string/title_activity_view_books" >
        </activity>
        <activity
            android:name=".RegActivity"
            android:label="@string/title_activity_reg" >
        </activity>
        <activity
            android:name=".LogActivity"
            android:label="@string/title_activity_log" >
        </activity>
        <activity
            android:name=".BookActivity"
            android:label="@string/title_activity_book" >
        </activity>
        <activity
            android:name=".BookInfoActivity"
            android:label="@string/title_activity_book_info" >
        </activity>
        <activity
            android:name=".OnlineLibMenuActivity"
            android:label="@string/title_activity_online_lib_menu" >
        </activity>
        <activity
            android:name=".onlinelibactivity"
            android:label="@string/title_activity_onlinelibactivity" >
        </activity>
        <activity
            android:name=".OnlineBookSelectActivity"
            android:label="@string/title_activity_online_book_select" >
        </activity>
    </application>

</manifest>
